 local wy=40
 for i=0,15 do
  for j=0,7 do
   local c=sget(wx+i,wy+j)
   local s
   if sget(wx+i-1,wy+j)>0 then
    s=83
    if sget(wx+i,wy+j-1)>0 then
     s=84
    elseif sget(wx+i,wy+j+1)>0 then
     s=68
    end
   elseif sget(wx+i+1,wy+j)>0 then
    s=83
    if sget(wx+i,wy+j-1)>0 then
     s=82
    elseif sget(wx+i,wy+j+1)>0 then

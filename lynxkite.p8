function draw_metaverse()
 --cyber background
 cls(1)
 local tt=t()/4
 local s=sin(tt/3)*30+160
 local vx=s*sin(tt/8)/2
 for i=1,100 do
  local p=(i+tt)*0.01
  line(
   s*sin(p*3)+vx+64,0,
   s*cos(p*7)+vx+64,96,
   i%3+5)
  line(
   s*sin(p*3)+vx+65,0,
   s*cos(p*7)+vx+65,96,
   i%3+5)
 end
 pal()
 --whiteboard
 local c,w=7,5
 rectfill(10,10,117,85,c)
 for x=1,w do
  line(10,10,10-x,85,c)
  line(117,10,117+x,85,c)
 end
 line(11,9,116,9,0)
 line(10,10,9-w,85,0)
 line(117,10,118+w,85,0)
 line(10-w,86,117+w,86,0)
 spr(28,20,87)
 spr(28,99,87)

function slot(x,y,data,wires)
 return {
  x=x,y=y,data=data,
  orig_data=data,wires=wires,
  draw=slot_draw,
  interact=slot_interact,
 }
end
function slot_draw(s)
 local x,y=s.x,s.y
 sh(96+tonum(s.data!=nil),x,y+2)
 if s.data!=nil then
  cubepal(s.data)
  spr(97,x,y+2)
 else
  spr(96,x,y+2)
 end
 pal()
end
function slot_interact(s)
 dialog="looks like a memory location into which a data cube fits."
 if s.data==nil and p.data!=nil then
  dialog="you place the data cube into the memory location."
  s.data=p.data
  p.data=nil
  sfx(23,-1,4*(s.data-10),4)
 elseif s.data!=nil and p.data==nil then
  dialog="you take the data cube from the memory location."
  p.data=s.data
  s.data=nil
  sfx(22,-1,4*(p.data-10),4)
 elseif s.data!=nil and p.data!=nil then
  dialog="this memory location is full."
 end
 score=getscore()
end

function getscore()
 sc=0
 n=#slots-1
 for i=1,n do
  --daniel
  if slots[i].data==slots[n-i+1].orig_data then
   sc+=1
  end
  --dog
  if slots[i].data!=nil
   and (slots[i].data==slots[i].wires[1]
   or slots[i].data==slots[i].wires[2]) then
   sc+=1
  end
  --cat
  if (i<n and slots[i].data==slots[i+1].data)
   or (i>1 and slots[i].data==slots[i-1].data) then
   sc-=1
  end
 end
 return max(0,sc)
end

function ssh(sx,sy,sw,sh,dx,dy)
 allblack()
 sspr(sx,sy,sw,sh,dx-1,dy)
 sspr(sx,sy,sw,sh,dx+1,dy)
 sspr(sx,sy,sw,sh,dx,dy-1)
 sspr(sx,sy,sw,sh,dx,dy+1)
 pal()
 sspr(sx,sy,sw,sh,dx,dy)
end

function fancynumbers(n,x,y)
 local s=tostr(n).."00"
 if n==0 then s="0" end
 x-=#s*6+(#s-1)\3*3
 for i=1,#s do
  local c=tonum(sub(s,i,i))
  if i>1 and (#s-i)%3==2 then
   ssh(51,56,3,8,x,y+1)
   x+=3
  end
  ssh(c*5,56,5,8,x,y)
  x+=6
 end
end

function cubepal(c)
 if c==10 then
  pal({[7]=10,[6]=4})
 elseif c==11 then
  pal({[7]=11,[6]=3})
 elseif c==12 then
  pal({[7]=12,[6]=1})
 elseif c==14 then
  pal({[7]=8,[6]=2})
 end
end

--by @mxsophieh
--word wrap(string, max width)
function wwrap(s,w)
 local ret,len,wrds,wrd = "",0,strspl(s," "),""
 for k=1, #wrds do
  wrd=strspl(wrds[k],"\n")
  for n=1,#wrd do
   if len+#wrd[n]>w or n>1 then
    ret=ret.."\n"
    len=0
   end
   ret=ret..wrd[n].." "
   len+=#wrd[n]+1
  end
 end
 if sub(ret,#ret-1,#ret-1)=="‚ùé" then
  ret=sub(ret,1,#ret-2)
  for k=1,w-len do
   ret=ret.." "
  end
  ret=ret.."‚ùé"
 end
 return ret
end
function strspl(s,sep)
 local ret, bffr = {},""
 for i=1, #s do
  if (sub(s,i,i-1+#sep)==sep)then
   add(ret,bffr)
   bffr=""
   i+=#sep-1
  else
   bffr = bffr..sub(s,i,i)
  end
 end
 if (bffr!="") add(ret,bffr)
 return ret
end
